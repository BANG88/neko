{
  "files.exclude": {
    "**/*.js.map": true,
    "**/*.js": {
      "when": "$(basename).tsx"
    }
  },
  "editor.formatOnSave": true,

  "files.associations": {
    ".env.prod": "dotenv",
    ".env.dev": "dotenv",
    ".env.prod.secret": "dotenv",
    ".env.dev.secret": "dotenv"
  },
  "git.ignoreLimitWarning": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  }
}
